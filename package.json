{
  "name": "app-logistic-company-api",
  "version": "0.0.1",
  "private": true,
  "namespace": "app-logistic-company",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "tsnd --inspect --transpile-only --ignore-watch node_modules src/index.ts",
    "start": "node index.js",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test": "jest --coverage=true",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:gen": "yarn typeorm migration:generate -f ./src/database/ormconfig.ts -d ./src/database/migrations/ -n",
    "migration:run:dev": "yarn typeorm migration:run -f ./src/database/ormconfig.ts",
    "migration:revert": "yarn typeorm migration:revert -f ./src/database/ormconfig.ts",
    "migration:run": "node_modules/typeorm/cli.js -f ormconfig.js migration:run"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/platform-fastify": "^8.4.4",
    "@nestjs/swagger": "^5.1.0",
    "@nestjs/typeorm": "8.0.3",
    "@sentry/node": "^6.19.2",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "fastify-static": "^4.7.0",
    "fastify-swagger": "^5.2.0",
    "ioredis": "^5.2.4",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
